<div class="title-section">
  <div class="row">
    <div class="col-lg-12">
      <h2>Outras Graduações</h2>
    </div>
  </div>
</div>

<div class="row">
  <div class="col-md-12">
    <ul class="list-group well">
      <% @institution.graduations.order('name ASC').to_a.uniq.each do |graduation| %>
        <% if !@institution.graduations.blank? %> 
          <li class="list-group-item">
            <%= link_to graduation.name, graduation_about_path(graduation.url_friendly) %>
          </li>
        <% end %>
      <% end %>
      </ul>
  </div>
</div>